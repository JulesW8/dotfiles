{
    "layer": "top",
    "position": "top",
    "margin": 0,
    "padding": 0,
    "modules-left": ["sway/workspaces"],
    "modules-center": ["clock"],
    "modules-right": ["network", "custom/launcher", "custom/power"],

    "sway/workspaces": {
	"disable-scroll": true,
        "all-outputs": true,
	"format": "{icon}",
	"format-icons": {
	  "1": "\uea7a",
	  "2": "\uea7a"
	}
    },

    "clock": {
        "format": "{:%d-%m-%Y %H:%M}",
        "tooltip-format": "{:%A, %d %B %Y}"
    },

    "network": {
        "interface": "wlp0s20f3",
        "format": "{icon}",
	"format-icons": ["\udb82\udd2f", "\udb82\udd1f", "\udb82\udd22", "\udb82\udd25", "\udb82\udd28"],
	"format-disconnected": "\udb82\udd2d",
	"tooltip-format": "{essid} | {signalStrength}% | {bandwidthTotalBytes}",
	"tooltip-disconnected": "Disconnected",
	"on-click": "nm-connection-editor",
	"interval": "180",
	"nospacing": 1
    },

    "custom/launcher": {
	"format": "\udb81\udc63",
	"tooltip": "Application Launcher",
	"on-click": "wofi --show drun"
    },

    "custom/power": {
	"format": "\udb81\udc25",
	"on-click": "wlogout -p layer-shell",
	"tooltip": false
    }
}
